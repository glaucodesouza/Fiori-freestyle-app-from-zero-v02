<mvc:View
    controllerName="freestyledozerov02.controller.Object"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<Page id="_IDGenPage1"
 	title="Produto"
 	showNavButton="true"
 	navButtonPress=".onNavBack"
 	floatingFooter="true">
        <f:SimpleForm id="_IDGenSimpleForm1"
            editable="true"
            layout="ResponsiveGridLayout"
            title="Produto"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsXL="1"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
            >

                <Label id="_IDGenLabel1" text="Codigo" />         <Input id="inpCodigo"       value="{Codigo}"    enabled="false"/>
                <Label id="_IDGenLabel2" text="Descricao" />      <Input id="inpDescricao"    value="{Descricao}" enabled="false"/>
                <Label id="_IDGenLabel3" text="Quantidade" />     <Input id="inpKwmeng"       value="{Kwmeng}"    enabled="false"/>
                <Label id="_IDGenLabel4" text="UM" />             <Input id="inpMeins"        value="{Meins}"     enabled="false"/>
                <Label id="_IDGenLabel5" text="Preço Unitário" /> <Input id="inpNetpr"        value="{Netpr}"     enabled="false"/>
                <Label id="_IDGenLabel6" text="Moeda" />          <Input id="inpWaerk"        value="{Waerk}"     enabled="false"/>

        </f:SimpleForm>

            <footer>
                <OverflowToolbar id="_IDGenOverflowToolbar1">
                    <ToolbarSpacer id="_IDGenToolbarSpacer1"/>
                    <Button id="_IDGenButton1" type="Emphasized"   text="Modificar"    press="onModificar" />
                </OverflowToolbar>
            </footer>
 	</Page>
</mvc:View>